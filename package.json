{
  "name": "ntn-try-me",
  "version": "1.0.9",
  "description": "Eliminate regular `try-catch-finally`",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Netanel Tal Nizri",
  "license": "MIT",
  "scripts": {
    "npm-hub": "node --experimental-modules --no-warnings build/npm-hub",
    "prebuild": "tsc",
    "build": "tsc",
    "---------------------------------": "",
    "minify": "npm run npm-hub -- --type=minify",
    "bundle": "npm run npm-hub -- --type=bundle",
    "bundleDev": "npm run npm-hub -- --type=bundleDev",
    "pretest": "npm run build && npm run bundleDev",
    "test": "jasmine --config=jasmine.json",
    "bump": "npm version patch && git push -u origin master && npm publish"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "Mini",
    "Helper",
    "Utility",
    "Tiny",
    "front-end",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "github:netanel-tn/try-me"
  },
  "devDependencies": {
    "@types/jasmine": "3.5.9",
    "@types/node": "13.9.0",
    "@types/webpack": "4.41.7",
    "npm": "6.14.2",
    "moment": "2.24.0",
    "terser": "4.6.6",
    "webpack": "4.42.0",
    "rxjs": "6.5.4",
    "typescript": "3.8.3",
    "jasmine": "3.5.0",
    "colors": "1.4.0"
  }
}
