{
  "name": "ntn-try-me",
  "version": "2.0.4",
  "description": "Eliminate regular `try-catch-finally`",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Netanel Tal Nizri",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "npm-hub": "node --experimental-modules --experimental-specifier-resolution=node --no-warnings build/npm-hub",
    "minify": "npm run npm-hub -- --type=minify",
    "bundle": "npm run npm-hub -- --type=bundle",
    "bundleDev": "npm run npm-hub -- --type=bundleDev",
    "pretest": "npm run build && npm run bundleDev",
    "test": "jasmine --config=jasmine.json",
    "bump": "npm version patch && git push -u origin master && npm publish"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "Mini",
    "Helper",
    "Utility",
    "Tiny",
    "front-end",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "github:netanel-tn/try-me"
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/jasmine": "3.6.3",
    "@types/node": "14.14.27",
    "@types/npm": "2.0.31",
    "npm": "7.5.4",
    "moment": "2.29.1",
    "terser": "5.6.0",
    "webpack": "5.21.2",
    "rxjs": "6.6.3",
    "typescript": "4.1.5",
    "jasmine": "3.6.4",
    "colors": "1.4.0"
  }
}
