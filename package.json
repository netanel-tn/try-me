{
  "name": "ntn-try-me",
  "version": "1.0.4",
  "description": "Eliminate regular `try-catch-finally`",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Netanel Tal Nizri",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "hub": "node --experimental-modules --no-warnings bin/npm-hub/npm-hub",
    "minify": "npm run hub -- --type=minify",
    "bundle": "npm run hub -- --type=bundle",
    "bundleDev": "npm run hub -- --type=bundleDev",
    "test": "jasmine --no-color --config=jasmine.json",
    "bump": "npm version patch & git push -u origin master & npm publish"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "Mini",
    "Helper",
    "Utility",
    "Tiny",
    "front-end",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "github:netanel-tn/try-me"
  },
  "devDependencies": {
    "@types/node": "13.7.0",
    "@types/jasmine": "3.5.3",
    "@types/webpack": "4.41.5",
    "moment": "2.24.0",
    "terser": "4.6.3",
    "webpack": "4.41.5",
    "rxjs": "6.5.4",
    "typescript": "3.7.5",
    "jasmine": "3.5.0",
    "colors": "1.4.0"
  }
}
